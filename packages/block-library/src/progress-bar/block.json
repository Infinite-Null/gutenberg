{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "core/progress-bar",
	"category": "design",
	"title": "Progress Bar",
	"description": "Display a progress bar. Useful for tracking progress on a task or project.",
	"textdomain": "default",
	"attributes": {
		"label": {
			"type": "string",
			"default": "Progress"
		},
		"value": {
			"type": "number",
			"default": 50
		},
		"max": {
			"type": "number",
			"default": 100
		},
		"backgroundColor": {
			"type": "string"
		},
		"progressColor": {
			"type": "string"
		}
	},
	"supports": {
		"align": true,
		"html": false
	},
	"style": "wp-block-progress-bar"
}
