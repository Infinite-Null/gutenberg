{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "core/progress-bar",
	"category": "design",
	"title": "Progress Bar",
	"description": "Display a progress bar. Useful for tracking progress on a task or project.",
	"textdomain": "default",
	"attributes": {
		"label": {
			"type": "string",
			"default": ""
		},
		"value": {
			"type": "number",
			"default": 50
		},
		"max": {
			"type": "number",
			"default": 100
		},
		"backgroundColor": {
			"type": "string",
			"default": "#f0f0f0"
		},
		"progressColor": {
			"type": "string",
			"default": "#1E1E1E"
		},
		"height": {
			"type": "number",
			"default": 11
		},
		"showValue": {
			"type": "boolean",
			"default": true
		},
		"isReadProgress": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"html": false
	},
	"style": "wp-block-progress-bar"
}
